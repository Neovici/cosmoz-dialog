{
	"name": "@neovici/cosmoz-dialog",
	"version": "3.0.2",
	"description": "A Polymer component workaround for paper-dialog that fits the backdrop to the element specified by the `fitInto` property.",
	"keywords": [
		"polymer",
		"web-components"
	],
	"homepage": "https://github.com/neovici/cosmoz-dialog#readme",
	"bugs": {
		"url": "https://github.com/neovici/cosmoz-dialog/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/neovici/cosmoz-dialog.git"
	},
	"license": "Apache-2.0",
	"author": "Neovici Development",
	"main": "cosmoz-dialog.js",
	"directories": {
		"test": "test"
	},
	"scripts": {
		"lint": "eslint --cache --ext .js .",
		"start": "wds",
		"test": "wtr --coverage",
		"test:watch": "wtr --watch",
		"prepare": "husky install"
	},
	"release": {
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/changelog",
			"@semantic-release/github",
			"@semantic-release/npm",
			"@semantic-release/git"
		],
		"branch": "master"
	},
	"publishConfig": {
		"access": "public"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"dependencies": {
		"@polymer/iron-form": "^3.0.0",
		"@polymer/paper-dialog": "^3.0.0",
		"@polymer/polymer": "^3.0.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.0.0",
		"@commitlint/config-conventional": "^17.0.0",
		"@neovici/cfg": "^1.17.1",
		"@polymer/paper-button": "^3.0.0",
		"@semantic-release/changelog": "^6.0.0",
		"@semantic-release/git": "^10.0.0",
		"@webcomponents/webcomponentsjs": "^2.0.0",
		"husky": "^8.0.0",
		"semantic-release": "^19.0.0",
		"sinon": "^14.0.0"
	}
}
