{
	"name": "@neovici/cosmoz-dialog",
	"version": "3.1.0",
	"description": "A Polymer component workaround for paper-dialog that fits the backdrop to the element specified by the `fitInto` property.",
	"keywords": [
		"polymer",
		"web-components"
	],
	"homepage": "https://github.com/Neovici/cosmoz-dialog#readme",
	"bugs": {
		"url": "https://github.com/Neovici/cosmoz-dialog/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Neovici/cosmoz-dialog.git"
	},
	"license": "Apache-2.0",
	"author": "Neovici Development",
	"main": "cosmoz-dialog.js",
	"directories": {
		"test": "test"
	},
	"scripts": {
		"lint": "eslint --cache .",
		"start": "wds",
		"test": "wtr --coverage",
		"test:watch": "wtr --watch",
		"prepare": "husky"
	},
	"release": {
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/changelog",
			"@semantic-release/github",
			"@semantic-release/npm",
			"@semantic-release/git"
		],
		"branch": "master",
		"preset": "conventionalcommits"
	},
	"publishConfig": {
		"access": "public"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		],
		"rules": {
			"body-max-line-length": [
				1,
				"always",
				600
			]
		}
	},
	"dependencies": {
		"@polymer/iron-form": "^3.0.0",
		"@polymer/paper-dialog": "^3.0.0",
		"@polymer/polymer": "^3.0.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^20.0.0",
		"@commitlint/config-conventional": "^20.0.0",
		"@neovici/cfg": "^2.0.0",
		"@open-wc/testing": "^4.0.0",
		"@polymer/iron-demo-helpers": "^3.1.0",
		"@polymer/paper-button": "^3.0.0",
		"@semantic-release/changelog": "^6.0.0",
		"@semantic-release/git": "^10.0.0",
		"@webcomponents/webcomponentsjs": "^2.0.0",
		"husky": "^9.1.7",
		"semantic-release": "^25.0.2"
	}
}
